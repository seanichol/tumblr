<script type="text/javascript">
$(function(){
var stickyRibbonTop = $('#sidebar').offset().top;
$(window).scroll(function(){
if( $(window).scrollTop() > stickyRibbonTop ) {
$('#sidebar').css({position: 'fixed', top: '0px'});
} else {
$('#sidebar').css({position: 'static', top: '0px'});
}
});
});
</script>

